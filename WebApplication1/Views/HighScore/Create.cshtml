@model WebApplication1.Models.HighScoreModel
@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Html.BeginForm())
{
    <div class="container">
        <div id="newForm" class="panel-group panel-primary">
            <h3>Creat High Score</h3>
            <hr/>
            <div class="panel-body">
                <span class="required text-danger">*</span>
                @Html.LabelFor(m => m.Name)
                @Html.TextBoxFor(m => m.Name, new {@class = "form-control"})
                @Html.ValidationMessageFor(m => m.Name, "", new {@class = "text-danger"})
                <br/>
                <span class="required text-danger">*</span>
                @Html.LabelFor(m => m.Score)
                @Html.TextBoxFor(m => m.Score, new {@class = "form-control"})
                @Html.ValidationMessageFor(m => m.Score, "", new {@class = "text-danger"})
                <br/>
                <input type="submit" value="Create" class="btn btn-info"/>
            </div>
        </div>
    </div>
}

        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
